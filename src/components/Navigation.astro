---
const navLinks = [
  { name: "home/", url: "/" },
  { name: "about/", url: "/about" },
  { name: "teaching/", url: "/teaching" },
  { name: "blog/", url: "/blog" },
  { name: "gallery/", url: "/gallery" },
  { name: "tags/", url: "/tags" },
];
---

<nav class="terminal-menu nav-links">
  <ul vocab="https://schema.org/" typeof="BreadcrumbList">
    {navLinks.map((link, index) => (
      <li>
        <a href={link.url} typeof="ListItem">{link.name}</a>
      </li>
    ))}
  </ul>
</nav>